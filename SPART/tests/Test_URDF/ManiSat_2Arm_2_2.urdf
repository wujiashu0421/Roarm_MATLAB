<?xml version="1.0" ?>

<!-- Simplistic Version of ManiSat for testing purposes -->

<robot name="ManiSat">

  <link name="Base-Spacecraft">
    <inertial>
      <mass value="13.3"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
