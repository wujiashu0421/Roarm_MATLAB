  </link>

  <joint name="JA" type="revolute">
    <origin rpy="0 0 1.5708" xyz="0 0.2 0"/>
    <axis xyz="0 0 1"/>
    <parent link="Base-Spacecraft"/>
    <child link="LA"/>
  </joint>

  <link name="LA">
    <inertial>
      <mass value="2.9"/>
