  </link>

  <joint name="JD" type="revolute">
    <origin rpy="0 0 0" xyz="0.3852 0 0"/>
    <axis xyz="0 0 1"/>
    <parent link="LC"/>
    <child link="LD"/>
  </joint>

  <link name="LD">
    <inertial>
      <mass value="2.9"/>
      <origin rpy="0 0 0" xyz="0.1635 0 0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.1245"/>
